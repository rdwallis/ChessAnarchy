<?xml version='1.0' encoding='UTF-8'?>
<module rename-to='chessanarchy'>
	<!-- Inherit the core Web Toolkit stuff. -->
	<inherits name='com.google.gwt.user.User' />
  
	<inherits name='com.google.gwt.user.theme.clean.Clean' />
	<inherits name='com.allen_sauer.gwt.dnd.gwt-dnd' />
	<inherits name='com.gwtplatform.dispatch.Dispatch' />
	<inherits name='com.gwtplatform.mvp.MvpWithEntryPoint' />
	<inherits name='com.google.gwt.appengine.channel.Channel' />
	<inherits name="com.google.gwt.http.HTTP" />

	<source path='client' />
	<source path='shared' />

	 <!--  <set-property name="user.agent" value="safari"></set-property>-->
	
	<add-linker name="xsiframe"/>

	<!-- Default client module -->
	<set-configuration-property name="gin.ginjector.modules"
		value="com.wallissoftware.chessanarchy.client.gin.ClientModule" />

	<!-- Bootstrapper -->
	<set-configuration-property name="gwtp.bootstrapper"
		value="com.wallissoftware.chessanarchy.client.gin.BootstrapperImpl" />

	 <inherits name="com.google.gwt.logging.Logging" />
	<set-property name="gwt.logging.logLevel" value="INFO" />
	<set-property name="gwt.logging.enabled" value="TRUE" />
 
  
	<set-property name="gwt.logging.simpleRemoteHandler"
		value="DISABLED" />
	<set-property name="gwt.logging.developmentModeHandler"
		value="ENABLED" />
	<set-property name="gwt.logging.systemHandler" value="ENABLED" />
	<set-property name="gwt.logging.popupHandler" value="DISABLED" />
	<set-property name="gwt.logging.consoleHandler" value="ENABLED" />
	<set-property name="gwt.logging.firebugHandler" value="DISABLED" />
  
 
    


    
</module>
