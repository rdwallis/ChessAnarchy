<!-- Defines the user.agent property and its provider function.             -->
<!--                                                                        -->
<!-- This module is typically inherited via com.google.gwt.user.User        -->
<!--                                                                        -->
<module type="fileset">
  <source path="client" />
  <public path="public" />

  <!-- Browser-sensitive code should use the 'user.agent' property -->
  <property-provider name="user.agent" generator="com.wallissoftware.useragent.rebind.UserAgentPropertyGenerator"/>


  <generate-with class="com.wallissoftware.useragent.rebind.UserAgentGenerator">
    <when-type-assignable class="com.wallissoftware.useragent.client.UserAgent" />
  </generate-with>
  
   <set-configuration-property name="user.agent.runtimeWarning" value="false"/>

</module>
